<p id="notice"><%= notice %></p>
<h3>
  Categoría: <br>
 <strong> <%= @category.name %> </strong>
</h3>
  
 <% @category.subcategories.each do |subcat| %>
 Subcategoría: <br>
 <h4><%= subcat.name %></h4>
 <table>
  <thead>
    <tr>
      <th>Marcadores</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @category.bookmarks.each do |bookmark| %>
      <tr>
        <td><ul><li><%= bookmark.url %> </li></ul>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
    <%end%>


<br>


<%= link_to 'Edit', edit_category_path(@category) %> |
<%= link_to 'Back', categories_path %>
